{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@sk/tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],      
    },
    "target": "es2017",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
  },
  "include": ["custom.d.ts", "next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
